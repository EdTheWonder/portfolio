  
{
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "SAMEORIGIN"
          },
          {
            "key" : "Content-Security-Policy",
            "value" : "default-src 'self' *.hotjar.com *.googleapis.com https://www.google-analytics.com  https://use.fontawesome.com;"
          }
        ]
      }
    ]
  }